<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="DLS-Elkarlana" name="DLS Elkarlana" >
		<settings>
			<setting configurable="true" key="is-login-page" type="select" options="yes,no" value="no"></setting>
		</settings>
		<color-scheme id="01" name="Classic">
			<default-cs>true</default-cs>
			<css-class>so-classic</css-class>
		</color-scheme>
		<color-scheme id="02" name="Strata">
			<css-class>so-strata</css-class>
		</color-scheme>
	</theme>
</look-and-feel>